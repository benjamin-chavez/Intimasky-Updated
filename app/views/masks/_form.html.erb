<%= simple_form_for @mask do |f|%>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price, label: 'Price per day' %>
  <%= f.input :address %>
  <%= f.input :category, collection: CATEGORIES %>
  <%= f.input :photo, as: :file %>
  <%= f.submit :class => "submit-button"%>
<% end %>
